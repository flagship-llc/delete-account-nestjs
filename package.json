  "scripts": {
    "heroku-prebuild": "npm install && nest build && npm run build:client",
    "prebuild": "rimraf dist",
    "build:client": "NODE_ENV=production parallel-webpack",
    "build": "nest build && npm run build:client",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:client": "NODE_ENV=development parallel-webpack --watch --progress",
